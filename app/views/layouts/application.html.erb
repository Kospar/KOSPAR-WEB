<style>
  li.mobile-nav{
    border-radius:10px;
    float:left;
    min-width:90px;
    max-width:90px;
    max-height:90px;
    min-height:90px;
    text-align:center;
    border:1px dotted #ddd;
    margin-bottom:10px;
    margin-left:10px;
  }
  li.mobile-nav:hover{
    border:1px solid #db3445;
    background-color:#db3445;
  }
  li.mobile-nav > a{
    vertical-align: middle;
    display: inline-block;
    line-height: normal;
    margin-top: 25px;
    text-align:center;
  }
  li.mobile-nav:hover > a{
    color:white;
    border-bottom: 0;
  }
  li.mobile-nav:nth-child(5) > a{
    margin-top: 15px;
    line-height:20px;
  }
  li.mobile-nav:nth-child(6) > a{
    margin-top: 15px;
    line-height:20px;
    padding-left:9px;
    padding-right:10px;
  }
  li.mobile-nav:nth-child(7) > a{
    margin-top: 15px;
    line-height:20px;
  }
  li.mobile-nav:nth-child(8) > a{
    margin-top: 15px;
    line-height:20px;
  }
  ul#mobile-category{
    padding-left:10%;
    padding-right:5%;
    text-align:center;
    margin: auto;
  }
  ul#mobile-category > hr{
    margin-left:-100%;
    margin-right:-100%;
    margin-bottom:10px;
    margin-top:0;
  }
  @media (max-width: 375px) {
    ul#mobile-category{
      padding-left:6%;
      padding-right:5%;
    }
    li.mobile-nav:nth-child(6) > a{
      margin-top: 13px;
      line-height:20px;
      padding-left:6px;
      padding-right:10px;
    }
  }
        
  @media (max-width: 360px) {
    ul#mobile-category{
      padding-left:3%;
      padding-right:5%;
    }
    li.mobile-nav:nth-child(6) > a{
      margin-top: 13px;
      line-height:20px;
      padding-left:4px;
      padding-right:10px;
    }
  }
  @media (max-width: 320px) {
    ul#mobile-category{
      padding-left:1%;
      padding-right:5%;
    }
    li.mobile-nav{
      float:left;
      min-width:80px;
      max-width:80px;
      max-height:80px;
      min-height:80px;
      text-align:center;
      border:1px dotted #ddd;
      margin-bottom:10px;
      margin-left:10px;
    }
    li.mobile-nav > a{
      vertical-align: middle;
      display: inline-block;
      line-height: normal;
      margin-top: 21px;
      text-align:center;
    }
    li.mobile-nav:nth-child(5) > a{
      margin-top: 13px;
      line-height:20px;
    }
    li.mobile-nav:nth-child(6) > a{
      margin-top: 13px;
      line-height:20px;
      padding-left:4px;
      padding-right:10px;
    }
    li.mobile-nav:nth-child(7) > a{
      margin-top: 13px;
      line-height:20px;
    }
    li.mobile-nav:nth-child(8) > a{
      margin-top: 13px;
      line-height:20px;
    }
  }
</style>
