  <%= csrf_meta_tags %>
  <style>
    ul#mobile-category{
      padding-left:10%;
      padding-right:5%;
      text-align:center;
      margin: auto;
      display:none;
    }
    .mce-primary{
        background-color:#db3445 !important;
    }
    li#home-btn{
      margin-right:10px;
    }
    li#my_page > a, li#logout > a{
      padding:0px;
    }
    .navbar-toggle {
      margin-top:6px;
    }
    .navbar-toggle .icon-bar {
      background-color: #606266;
    }
    nav.navbar{
      margin-bottom:0px;
      border-top: 3px solid #db3445;
      border-bottom: 2px solid #eee;
      padding-bottom:0px;
      border-radius:0;
      background-color:white;
    }
    .navbar-default{
      border-radius:0;  
    }
    nav a{
      color:#606266;
      font-family: sans-serif;
    }
    .nav > li > a{
      padding:10px;
      line-height:31px;
      font-weight:700;
    }
    .navbar-nav>li>a{
      background-color:transparent;
      border-bottom: 3px solid #ffffff;
      text-align:center;
    }
    .navbar-nav>li>ul{
      text-align:center;
      padding-left:0px;
    }
    .navbar-nav>li>a:hover{
      background-color:transparent;
      border-bottom: 3px solid #db3445;
      color:#db3445;
    }
    #underbar>li>a{
      background-color:transparent;
      border-bottom: 0px;
      padding-left:50px;
      padding-right:50px;
    }
    ul#underbar{
      float:none;
    }
    #underbar li{
      float:none;
      display:inline-block;
    }
    #underbar>li>a:hover{
      background-color:transparent;
      border-bottom: 0px;
    }

    .nav .open>a, .nav .open>a:focus, .nav .open>a:hover {
      background-color:transparent;
      border-bottom: 3px solid #db3445;
      color:#db3445;
    }
    .dropdown-menu>li>a {
      color: #606266;
      text-align:center;
      
    }
    ul.dropdown-menu{
      min-width:0px;
      padding-right:0px;
    }
    .dropdown-menu>li>a:hover {
      color:#db3445;
      background-color:transparent;
      background-image:none;
    }
    .navbar{
        padding-top: 10px;
        padding-bottom: 10px;
    }
    .navbar-default{
        box-shadow: none;
    }
    li.nav-mobile{
        display:none;
    }
    @media only screen and (max-device-width : 480px){
      .nav-mobile{
        display:block;
      }
      .dropdown.web{
        display:none;
      }
      img#logo_nav{
        height:25px;
        margin-top: -5px;
        margin-left:0px;
        padding-left:0px;
      }
      li.nav-mobile{
        display:block;
      }

    }
    #logo_nav{
      height:25px;
      margin-top: -5px;
      margin-left:-6px;
      padding-left:5px;
    }
    #web-category li{
      display:inline-block;
      margin-right: 30px;
      margin-left: 30px;
    }
    #web-category li:nth-child(3){
      margin-right: 20px;
      margin-left: 20px;
    }
    #web-category li:nth-child(4){
      margin-right: 10px;
      margin-left: 10px;
    }
    .nav> li:nth-child(3) > a{
      min-width:130px;
    }
    .nav > li:nth-child(4) > a{
      min-width:119px;
    }
    #web-category li > ul >li{
      display:block;
      list-style:none;
      margin-right: 5px;
      margin-left: 5px;
      margin-top:10px;
      margin-bottom:5px;
    }
    #web-category li > ul >li>a{
      color:white;
    }
    
    #web-category li > ul{
      margin-top:0px;
      padding-top:20px;
      display:none;
      min-height:150px;
      background-color:#db3445;
      z-index:10;
    }
    .navbar-right .dropdown-menu {
      right: -10px;
      left: -10px;
    }
    .ul-wrapper-1{
      display:inline-block;
      margin-left:259px;
      margin-top:20px;
    }
    #background{
      line-height:360px;
      background:#db3445;
      height:200px;
      margin-top:-155px;
      display:none;
    }
    #copyright{
      line-height:60px;
      vertical-align:middle;
      color:white;
    }
    html,body {
      margin:0;
      padding:0;
      height:100%;
    }
    .container{
      z-index:9;
    }
    footer{
      bottom:0;
      height:60px;
      background-color:#444;
      /*border-top:2px dotted #ddd;*/
      /*border-bottom:3px solid #db3445;*/
      width:100%;
    }
    
    .container{
      margin-top:80px;
      min-height:100%;
      position:relative;
    }
    /*navbar break-point*/
    @media (max-width: 1000px) {
      .nav> li:nth-child(3) > a{
        min-width:0px;
      }
      .nav > li:nth-child(4) > a{
        min-width:0px;
      }
      .navbar-brand{
        margin-right:0;
      }
      .mobile-nav{
        display:block;
      }
      header{
        margin-top: -5px;
      }
      #web-category li{
        display:block;
        margin-right: 30px;
        margin-left: 30px;
      }
      .navbar-nav>li>a:hover{
        background-color:transparent;
        border-bottom: 0;
      }
      .navbar-header {
          float: none;
      }
      .navbar-toggle {
          display: block;
      }
      .navbar-collapse {
          border-top: 1px solid transparent;
          box-shadow: inset 0 1px 0 rgba(255,255,255,0.1);
      }
      .navbar-collapse.collapse {
          display: none!important;
      }
      .navbar-collapse.collapse.in {
          display: block!important;
      }
      .navbar-nav {
          float: none!important;
          margin: 7.5px -15px;
      }
      .navbar-nav>li {
          float: none;
      }
      .navbar-nav>li>a {
          padding-top: 10px;
          padding-bottom: 10px;
      }
    }
    .navbar-brand{
      margin-right:50px;
    }
  </style>
</head>
<body>
  <nav class="navbar navbar-fixed-top" id="navbar">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/">
          <!--<img src="/logo_nav.png" ></img>-->
          <%= image_tag("logo_nav.png",:alt =>"logo_nav", :id =>"logo_nav") %>
        </a>
      </div>
  
      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-left" id="web-category">
            <li class="web-nav">
              <a>KOSPAR</a>
              <ul>
                <li><a href="/About_us">About us</a></li>
                <li><a href="/Notice">Notice</a></li>
              </ul>
            </li>
            <li class="web-nav">
              <a>Tech Trends</a>
              <ul>
                <li><a href="/trends/latest">Latest Trends</a></li>
              </ul>
            </li>
            <li class="web-nav">
              <a>Tech Standards</a>
              <ul>
                <li><a href="/Design_documents">Design Dcouments</a></li>
                <li><a href="/Opensrcs">Open Sources</a></li>
              </ul>
            </li>
            <li class="web-nav">
              <a>Board</a>
              <ul>
                <li><a href="/Q&A">Q & A</a></li>
                <li><a href="/forums">KOSPAR Forums</a></li>
              </ul>
            </li>
          </ul>
          <ul class="nav navbar-nav navbar-left" id="mobile-category">
            <hr>
            <li class="mobile-nav"><a href="/About_us">About us</a></li>
            <li class="mobile-nav"><a href="/Notice">Notice</a></li>
            <li class="mobile-nav"><a href="/Q&A">Q & A</a></li>
            <li class="mobile-nav"><a href="/trends/latest">Latest<br>Trends</a></li>
            <li class="mobile-nav"><a href="/Design_documents">Design<br>Dcouments</a></li>
            <li class="mobile-nav"><a href="/Opensrcs">Open<br>Sources</a></li>
            <li class="mobile-nav"><a href="/forums">KOSPAR<br>Forums</a></li>
            <% if user_signed_in? %>
              <% if current_user.admin? %>
                <li class="mobile-nav"><a style="margin-top:15px;line-height: 20px;"href="/admin_page">Admin<br>page</a></li>
              <% else %>
                <li class="mobile-nav"><a href="/mypage">My page</a></li>
              <% end %>
              <li class="mobile-nav"><a href="/users/sign_out">Log out</a></li><br>
            <% else %>
              <li class="mobile-nav"><a href="/users/sign_in">Log in</a></li><br>
            <% end %>
          </ul>
        <ul class="nav navbar-nav navbar-right" style="margin-right:15px;" id="nav-mobile-none">
          <li id="home-btn"><a href="/">Home</a></li>
          <% if user_signed_in? %>
            <li class="dropdown web">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Menu<span class="caret"></span></a>
              <ul class="dropdown-menu">
                <% if current_user.admin? %>
                  <li id="my_page"><a href="/admin_page" style="margin-top:5px;">Admin</a></li>
                  <li role="separator" class="divider"></li>
                  <li id="logout"><a href="/users/sign_out" style="margin-bottom:5px;">Log out</a></li>
                <% else %>
                  <li id="my_page"><a href="/mypage" style="margin-top:5px;">My page</a></li>
                  <li role="separator" class="divider"></li>
                  <li id="logout"><a href="/users/sign_out" style="margin-bottom:5px;">Log out</a></li>
                <% end %>
              </ul>
            </li>
          <% else %>
            <li><a href="/users/sign_in">Log in</a></li>
          <% end %>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
    <div id="background" class="text-center">
    </div>
  </nav>
  <%= tinymce :language => "ko" %>
  <%= yield %>
  <footer>
    <div class="container-fluid">
      <div class="text-center" id="copyright">
        Copyright &copy; KOSPAR 2017. All rights reserved
      </div>
    </div> 
  </footer>
</body>
<script>
  $("ul#web-category").mouseenter(function() {
    $("ul#web-category li ul").toggle();
    $("div#background").toggle();
  });
  $("ul#web-category li a").click(function() {
      $("ul#web-category li ul").toggle();
      $("div#background").toggle();
  });
  $("div#background").mouseout(function() {
      $("ul#web-category li ul").toggle();
      $("div#background").toggle();
  });
  $(document).ready(function(){
      if ($(window).width() > 1000) {
      // $("ul#web-category").mouseenter(function() {
      //   $("ul#web-category li ul").toggle();
      //   $("div#background").toggle();
      // });
      // $("ul#web-category li a").click(function() {
      //     $("ul#web-category li ul").toggle();
      //     $("div#background").toggle();
      // });
      // $("div#background").mouseout(function() {
      //     $("ul#web-category li ul").toggle();
      //     $("div#background").toggle();
      // });
      $("ul#mobile-category").css("display", "none");
      $("ul#web-category").css("display", "block");
      $("#nav-mobile-none").css("display", "block");
    } 
    else {
      $("ul#mobile-category").css("display", "block");
      $("ul#web-category").css("display", "none");
      $("ul#web-category li ul").css("display", "none");
      $("div#background").css("display", "none");
      $("#nav-mobile-none").css("display", "none");
  
    }
  });
  $( window ).resize(function() {
    if ($(window).width() > 1000) {
      // $("ul#web-category").mouseenter(function() {
      //   $("ul#web-category li ul").toggle();
      //   $("div#background").toggle();
      // });
      // $("ul#web-category li").click(function() {
      //     $("ul#web-category li ul").toggle();
      //     $("div#background").toggle();
      // });
      // $("div#background").mouseout(function() {
      //     $("ul#web-category li ul").toggle();
      //     $("div#background").toggle();
      // });
      $("ul#mobile-category").css("display", "none");
      $("ul#web-category").css("display", "block");
      $("#nav-mobile-none").css("display", "block");
    } 
    else {
      $("ul#mobile-category").css("display", "block");
      $("ul#web-category").css("display", "none");
      $("ul#web-category li ul").css("display", "none");
      $("div#background").css("display", "none");
      $("#nav-mobile-none").css("display", "none");
    }
  });
</script>
